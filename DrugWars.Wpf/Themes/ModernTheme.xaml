<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="AccentColor">#26C6DA</Color>
    <SolidColorBrush x:Key="WindowBackground" Color="#181A1B"/>
    <SolidColorBrush x:Key="TextForeground" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <FontFamily x:Key="FontFamily">Verdana</FontFamily>
    <SolidColorBrush x:Key="ButtonBackground" Color="#23272A"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#26C6DA"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#26C6DA"/>
    <SolidColorBrush x:Key="TileBackground" Color="#222"/>
    <SolidColorBrush x:Key="TileBorderBrush" Color="#26C6DA"/>
    <SolidColorBrush x:Key="StatusBarBackground" Color="#23272A"/>
</ResourceDictionary> 