<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="AccentColor">#FF9800</Color>
    <SolidColorBrush x:Key="WindowBackground" Color="#FFF9C4"/>
    <SolidColorBrush x:Key="TextForeground" Color="#1A237E"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <FontFamily x:Key="FontFamily">Comic Sans MS</FontFamily>
    <SolidColorBrush x:Key="ButtonBackground" Color="#FFE082"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#FF9800"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#FF9800"/>
    <SolidColorBrush x:Key="TileBackground" Color="#FFFDE7"/>
    <SolidColorBrush x:Key="TileBorderBrush" Color="#FF9800"/>
    <SolidColorBrush x:Key="StatusBarBackground" Color="#FFE082"/>
</ResourceDictionary> 