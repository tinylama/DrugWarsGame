<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="AccentColor">#00FF00</Color>
    <SolidColorBrush x:Key="WindowBackground" Color="#000000"/>
    <SolidColorBrush x:Key="TextForeground" Color="#00FF00"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <FontFamily x:Key="FontFamily">Consolas</FontFamily>
    <SolidColorBrush x:Key="ButtonBackground" Color="#0C1F0C"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#00FF00"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#00FF00"/>
    <SolidColorBrush x:Key="TileBackground" Color="#0C1F0C"/>
    <SolidColorBrush x:Key="TileBorderBrush" Color="#00FF00"/>
    <SolidColorBrush x:Key="StatusBarBackground" Color="#0C1F0C"/>
    <SolidColorBrush x:Key="ErrorForeground" Color="#FF4444"/>
    <SolidColorBrush x:Key="ErrorBorderBrush" Color="#FF2222"/>
    <SolidColorBrush x:Key="CancelButtonBackground" Color="#3c0000"/>
    <SolidColorBrush x:Key="CancelButtonBackgroundMouseOver" Color="#7F0000"/>
    <SolidColorBrush x:Key="ListBoxBackground" Color="Black"/>
    <SolidColorBrush x:Key="ListBoxForeground" Color="Lime"/>
    <SolidColorBrush x:Key="ListBoxBorderBrush" Color="Lime"/>
    <SolidColorBrush x:Key="ListBoxItemSelectedBackground" Color="#1A3300"/>
    <SolidColorBrush x:Key="ListBoxItemMouseOverBackground" Color="#0D1A00"/>
    <SolidColorBrush x:Key="ButtonBackgroundMouseOver" Color="#145214"/>
    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#0A2A0A"/>
</ResourceDictionary> 